.PHONY: src/all src/clean src/depclean

src/all: $(LIBRUMINATE)

src/clean:
	$(RM) $(SRC_OBJECTS) $(LIBRUMINATE) $(SRC_PYTHON_OBJECTS)

src/depclean:
	$(RM) $(SRC_DEPS)

$(LIBRUMINATE): $(SRC_OBJECTS) $(ICE_OBJECTS) $(ICE_PY_SOURCES)
$(LIBRUMINATE): SO_OBJECTS := $(SRC_OBJECTS) $(ICE_OBJECTS)
$(LIBRUMINATE): FLAGS_LINKER := $(SRC_FLAGS_LINKER) $(FLAGS_LINKER)

$(SRC_DEPS): $(ICE_HEADERS)

-include $(SRC_DEPS)
